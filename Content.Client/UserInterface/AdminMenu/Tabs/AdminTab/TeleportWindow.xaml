<SS14Window
    xmlns="https://spacestation14.io" Title="{Loc Teleport}">
    <VBoxContainer>
        <HBoxContainer>
            <Label Text="{Loc Player}" MinSize="100 0" />
            <Control MinSize="50 0" />
            <OptionButton Name="PlayerOptions" MinSize="100 0" HorizontalExpand="True" />
        </HBoxContainer>
        <Button Name="SubmitButton" Text="{Loc Teleport}" />
    </VBoxContainer>
</SS14Window>
