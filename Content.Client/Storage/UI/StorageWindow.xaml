<SS14Window
    xmlns="https://spacestation14.io"
    SetSize ="240 320"
    Title="{Loc 'comp-storage-window-title'}"
    RectClipContent="True">
    <ContainerButton Name="StorageContainerButton" MouseFilter="Pass">
        <PanelContainer Name="InnerContainerButton"/>
        <BoxContainer Name="InnerContainerBox" Orientation="Vertical" MouseFilter="Ignore">
            <Label Name="Information" VerticalAlignment="Center"/>
        </BoxContainer>
    </ContainerButton>
</SS14Window>
